{
	"name": "gulp-web-compress",
	"version": "0.1.0",
	"description": "A gulp plugin to optionally compress output with gzip and brotli",
	"main": "index.js",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava -v"
	},
	"engines": {
		"node": ">=6"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"index.js"
	],
	"keywords": [
		"gulp",
		"gulpplugin",
		"brotli",
		"gzip",
		"vinyl"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/gulp-web-compress.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/gulp-web-compress/issues"
	},
	"homepage": "https://github.com/cfware/gulp-web-compress#readme",
	"dependencies": {
		"arrify": "^1.0.1",
		"branch-pipe": "^1.0.1",
		"gulp-brotli": "^1.2.1",
		"gulp-clone": "^2.0.1",
		"gulp-filter": "^5.1.0",
		"gulp-gzip": "^1.4.2",
		"stream.pipeline-shim": "^1.0.4"
	},
	"devDependencies": {
		"ava": "^1.3.1",
		"concat-stream": "^2.0.0",
		"gulp": "^4.0.0",
		"nyc": "^13.3.0",
		"standard-version": "^5.0.1",
		"xo": "^0.24.0"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"*.js"
		]
	}
}
